<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoopViewPager">
        <attr name="loop_ms" format="integer" />
        <attr name="loop_duration" format="integer" />
        <attr name="loop_style" format="enum">
            <enum name="empty" value="-1"/>
            <enum name="depth" value="1"/>
            <enum name="zoom" value="2"/>
        </attr>
    </declare-styleable>
    <attr name="titleText" format="string"/>
    <attr name="titleTextSize" format="dimension"/>
    <attr name="titleColor" format="color"/>
    <attr name="titleBgColor" format="color"/>
    <attr name="iconExpanded" format="reference"/>
    <attr name="iconCollapse" format="reference"/>
    <attr name="iconColor" format="color"/>
    <attr name="titleHighLight" format="color"/>
    <declare-styleable name="DropdownMenu">
        <attr name="titleText"/>
        <attr name="titleTextSize"/>
        <attr name="titleColor"/>
        <attr name="titleBgColor"/>
        <attr name="iconExpanded"/>
        <attr name="iconCollapse"/>
        <attr name="iconColor"/>
        <attr name="listBgColor" format="color"/>
        <attr name="titleHighLight"/>
        <attr name="drawableLeft" format="reference"/>
        <attr name="drawablePadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="FixedHeightListView">
        <attr name="maxHeight" format="dimension"/>
    </declare-styleable>
</resources>